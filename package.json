{
  "name": "cssta",
  "version": "0.10.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "prepack": "tsc",
    "clean": "git clean -xf compiler runtime",
    "readme": "cp ./docs/index.md ./README.md; sed -i -e 's%{{ site.baseurl }}%https://jacobp100.github.io/cssta%g' ./README.md; sed -i -e 's/{% raw %}//g' ./README.md; sed -i -e 's/{% endraw %}//g' ./README.md; sed -i -e '1,5d' ./README.md; rm ./README.md-e"
  },
  "keywords": [
    "postcss",
    "modules",
    "css-modules",
    "css",
    "minify",
    "min",
    "class",
    "className",
    "react",
    "css-in-js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jacobp100/cssta"
  },
  "files": [
    "compiler/**/*.js",
    "runtime/**/*.js",
    "native.macro.js",
    "babel-plugin.js"
  ],
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/generator": "^7.7.4",
    "@babel/plugin-transform-modules-commonjs": "^7.7.5",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-typescript": "^7.7.4",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.17",
    "@types/react-test-renderer": "^16.9.1",
    "@typescript-eslint/eslint-plugin": "^2.11.0",
    "@typescript-eslint/parser": "^2.11.0",
    "babel-plugin-macros": "^2.8.0",
    "eslint": "^6.7.2",
    "jest": "^24.9.0",
    "prettier": "^2.2.1",
    "react": "^16.12.0",
    "react-test-renderer": "^16.12.0",
    "typescript": "^3.7.3"
  },
  "dependencies": {
    "@types/react": "16.9.5",
    "@types/react-native": "^0.60.25",
    "css-color-function": "^1.3.3",
    "css-to-react-native": "^3.0.0",
    "postcss": "^7.0.24",
    "postcss-selector-parser": "^6.0.2"
  },
  "jest": {
    "testRegex": "/__tests__/.*\\.ts?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
